from karel.stanfordkarel import *

def main():
   move()
   put_beeper_in_walls()
   move()
   move()
def put_beeper_in_walls():
    for i in range(3):
        turn_left()
        move()
        for j in range(2):
            turn_right()
            move()
        put_beeper()
        turn_left()
def turn_right():
    for i in range(3):
        turn_left()


# There is no need to edit code beyond this point

if __name__ == '__main__':
    main()
