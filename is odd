
def main():
    for i in range(10):
        #print(i)
        if is_odd(i):
            print('even')
        else:
            print('odd')
            
def is_odd(value):

    remainder = value % 2  # 0 if value is divisible by 2, 1 if it isn't
    return remainder == 0


# There is no need to edit code beyond this point

if __name__ == '__main__':
    main()
